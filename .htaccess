<IfModule mod_rewrite.c>
    RewriteEngine On
    RewriteBase /
    RewriteRule ^$ public/index.php [L]
    RewriteRule ^((?!public/).*)$ public/$1 [L,NC]
</IfModule>
RewriteEngine On
RewriteCond %{SERVER_PORT} 80
RewriteCond %{HTTP_HOST} atmo-ndash\.com$ [OR]
RewriteCond %{HTTP_HOST} ^www\.atmo-ndash\.com$
RewriteRule ^(.*)$ http://atmo-ndash.com%{REQUEST_URI} [R=301,L]